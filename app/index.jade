html(ng-app='app',dir='ltr')
  head
    // build:css(app) styles/vendor.css
    // bower:css
    link(rel='stylesheet', href='bower_components/semantic-ui/dist/semantic.css')
    link(rel='stylesheet', href='bower_components/angular-ui-layout/ui-layout.css')
    // endbower
    link(rel='stylesheet', href='bower_components/pdfjs-bower/dist/viewer.css')
    link(rel='stylesheet', href='scripts/timeline_2.3.1/timeline_js/timeline-bundle.css')
    // endbuild
    // build:css(.tmp) styles/main.css
    link(rel='stylesheet', href='styles/main.css')
    // endbuild
    link(rel='resource', type='application/l10n', href='bower_components/pdfjs-bower/dist/locale/locale.properties')
    script(type="text/javascript").
      Timeline_ajax_url="scripts/timeline_2.3.1/timeline_ajax/simile-ajax-api.js"
      Timeline_urlPrefix='scripts/timeline_2.3.1/timeline_js/'
      Timeline_parameters='bundle=true'
    script(type="text/javascript",src="scripts/timeline_2.3.1/timeline_js/timeline-api.js")
  body
    main.container(ui-view="")
    // build:js(app) scripts/vendor.js
    // bower:js
    script(src='bower_components/jquery/dist/jquery.js')
    script(src='bower_components/angular/angular.js')
    script(src='bower_components/angular-ui-router/release/angular-ui-router.js')
    script(src='bower_components/semantic-ui/dist/semantic.js')
    script(src='bower_components/pdfjs-bower/dist/compatibility.js')
    script(src='bower_components/pdfjs-bower/dist/pdf.js')
    script(src='bower_components/angular-google-staticmaps/angular-google-staticmaps.js')
    script(src='bower_components/ngmap/build/scripts/ng-map.js')
    script(src='bower_components/angular-sanitize/angular-sanitize.js')
    script(src='bower_components/raf/index.js')
    script(src='bower_components/angular-ui-layout/ui-layout.js')
    // endbower
    script(src="http://maps.google.com/maps/api/js?sensor=false")
    script(type="text/javascript",src='bower_components/xregexp/xregexp-all.js')
    script(type="text/javascript",src='bower_components/pdfjs-bower/dist/l10n.js')
    script(type="text/javascript",src="bower_components/pdfjs-bower/dist/viewer.js")
    // endbuild
    // build:js(.tmp) scripts/main.js
    script(src='scripts/sparql/sparqlService.js')
    script(src='scripts/prefix/prefixService.js')
    script(src='scripts/cors-proxy-interceptor/cors-proxy-interceptor.js')
    script(src="scripts/config.js")
    script(src="scripts/main/main-ctrl.js")
    script(src="scripts/main/ww1-configuration.js")
    // inject:partials
    // endinject
    // endbuild
